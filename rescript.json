{
  "name": "@illusionalsagacity/rescript-react-query",
  "namespace": "ReactQuery",
  "jsx": {
    "mode": "automatic",
    "version": 4
  },
  "bsc-flags": ["-open RescriptCore"],
  "bs-dependencies": [
    "@glennsl/rescript-fetch",
    "@rescript/core",
    "@rescript/react"
  ],
  "bs-dev-dependencies": [
    "@illusionalsagacity/rescript-msw",
    "rescript-vitest"
  ],
  "sources": [
    { "dir": "src", "subdirs": true },
    { "dir": "__test__", "subdirs": true, "type": "dev" },
    { "dir": "example", "subdirs": true, "type": "dev" }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true,
    "suffix": ".res.mjs"
  }
}
